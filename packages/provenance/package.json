{
	"name": "@tripleslate/provenance",
	"version": "0.7.2",
	"description": "server-side SvelteKit auth",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TripleslateSoftware/provenance.git",
		"directory": "packages/provenance"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/TripleslateSoftware/provenance/issues"
	},
	"homepage": "https://github.com/TripleslateSoftware/provenance",
	"keywords": [
		"sveltekit",
		"authentication",
		"oauth"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"prepublishOnly": "pnpm run build"
	},
	"peerDependencies": {
		"@sveltejs/kit": "^2.0.0",
		"vite": "^5.0.3"
	},
	"devDependencies": {
		"@types/cookie": "0.6.0",
		"@types/node": "^20.14.2",
		"dedent": "^1.5.3",
		"tslib": "^2.6.3",
		"tsup": "^8.1.0",
		"vite": "^5.3.1"
	},
	"dependencies": {
		"oauth4webapi": "^2.10.4"
	},
	"type": "module",
	"module": "./dist/index.js",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json",
		"./vite": {
			"types": "./dist/vite/index.d.ts",
			"import": "./dist/vite/index.js"
		},
		"./runtime/ts": {
			"default": "./src/vite/runtime/ts.ts"
		},
		"./runtime/js": {
			"types": "./src/vite/runtime/js.d.ts",
			"import": "./src/vite/runtime/js.js"
		},
		"./providers": {
			"types": "./dist/providers/index.d.ts",
			"import": "./dist/providers/index.js"
		},
		"./helpers": {
			"types": "./dist/helpers/index.d.ts",
			"import": "./dist/helpers/index.js"
		}
	},
	"typesVersions": {
		"*": {
			"vite": [
				"./dist/vite/index.d.ts"
			],
			"providers": [
				"./dist/providers/index.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"src/vite/runtime"
	]
}
