{
	"name": "@tripleslate/provenance",
	"version": "0.4.1",
	"description": "server-side SvelteKit auth",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TripleslateSoftware/provenance.git",
		"directory": "packages/provenance"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/TripleslateSoftware/provenance/issues"
	},
	"homepage": "https://github.com/TripleslateSoftware/provenance",
	"keywords": [
		"sveltekit",
		"authentication",
		"oauth"
	],
	"scripts": {
		"build": "rimraf dist && tsup",
		"dev": "tsup --watch",
		"prepare": "npm run build",
		"clean:all": "rimraf dist node_modules",
		"prepublishOnly": "npm run build"
	},
	"peerDependencies": {
		"@sveltejs/kit": "^2.0.0",
		"vite": "^5.0.3"
	},
	"devDependencies": {
		"@types/cookie": "0.5.1",
		"@types/node": "^20.4.9",
		"dedent": "^1.5.1",
		"rimraf": "^5.0.1",
		"tslib": "^2.6.1",
		"tsup": "^6.5.0",
		"vite": "^5.0.10"
	},
	"dependencies": {
		"oauth4webapi": "^2.3.0"
	},
	"type": "module",
	"module": "./dist/index.js",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json",
		"./vite": {
			"types": "./dist/vite/index.d.ts",
			"import": "./dist/vite/index.js"
		},
		"./providers": {
			"types": "./dist/providers/index.d.ts",
			"import": "./dist/providers/index.js"
		}
	},
	"typesVersions": {
		"*": {
			"vite": [
				"./dist/vite/index.d.ts"
			],
			"providers": [
				"./dist/providers/index.d.ts"
			]
		}
	},
	"files": [
		"dist"
	]
}
